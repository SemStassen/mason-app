{
  "name": "mason",
  "packageManager": "bun@1.2.5",
  "scripts": {
    "build": "turbo build",
    "clean": "git clean -xdf node_modules",
    "dev": "turbo dev --parallel",
    "docker:up": "docker compose -f ./docker-compose.yml up -d",
    "format": "biome format --write .",
    "lint": "turbo lint && manypkg check",
    "typecheck": "turbo typecheck"
  },
  "private": true,
  "workspaces": [
    "interface",
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "@effect/platform": "^0.90.3",
    "@effect/platform-bun": "^0.79.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "better-auth": "1.3.4",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "effect": "^3.17.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@effect/language-service": "^0.38.1",
    "lefthook": "^1.12.4",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "ultracite": "5.3.4"
  },
  "patchedDependencies": {
    "better-auth@1.3.4": "patches/better-auth@1.3.4.patch"
  }
}
