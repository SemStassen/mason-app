{
  "name": "mason",
  "packageManager": "bun@1.3.0",
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean --parallel",
    "dev": "turbo dev --parallel",
    "docker:up": "docker compose -f ./docker-compose.yml up -d",
    "format": "biome format --write .",
    "lint": "turbo lint && manypkg check",
    "typecheck": "turbo typecheck",
    "subtree:create:effect": "git remote add effect https://github.com/effect-ts/effect && git subtree add --prefix=.better-agents/effect effect main --squash",
    "subtree:update:effect": "git subtree pull --prefix=.better-agents/effect effect main --squash"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "interface",
      "apps/*",
      "packages/*",
      "packages/core/*",
      "packages/core/modules/*"
    ],
    "catalog": {
      "@effect/platform": "^0.93.0",
      "@effect/sql": "^0.49.0",
      "@effect/sql-drizzle": "^0.48.0",
      "@effect/sql-pg": "^0.50.0",
      "effect": "^3.19.3"
    },
    "catalogs": {
      "dev": {
        "@tailwindcss/vite": "^4.1.17",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.1",
        "@vitejs/plugin-react": "^5.0.4",
        "babel-plugin-react-compiler": "^1.0.0",
        "drizzle-kit": "^0.31.8",
        "typescript": "^5.9.3",
        "vite": "^7.2.2",
        "vite-tsconfig-paths": "^5.1.4"
      },
      "frontend": {
        "@base-ui-components/react": "^1.0.0-beta.4",
        "@tanstack/react-form": "^1.23.0",
        "drizzle-orm": "^0.44.4",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
      },
      "server": {
        "drizzle-orm": "^0.45.1"
      }
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@effect/language-service": "^0.62.4",
    "lefthook": "^2.0.12",
    "turbo": "^2.7.1",
    "typescript": "catalog:dev",
    "ultracite": "6.5.0"
  },
  "dependencies": {}
}
