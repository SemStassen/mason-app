{
  "name": "@mason/sdk",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "format": "biome format --write .",
    "lint": "biome check .",
    "typecheck": "tsc -noEmit",
    "pregenerate": "bun run ./scripts/generate",
    "generate": "openapi-generator-cli generate -i spec/openapi.json -g typescript-fetch -o src/generated-client --additional-properties=supportsES6=true,useSingleRequestParameter=false,useUnionTypes=true"
  },
  "dependencies": {},
  "devDependencies": {
    "@mason/config": "workspace:*",
    "@openapitools/openapi-generator-cli": "^2.21.4",
    "typescript": "workspace:^"
  },
  "exports": {
    "./mason-client": "./src/mason-client.ts"
  }
}
