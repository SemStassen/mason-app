    "/api/auth/email-otp": {
      "post": {
        "tags": [
          "Auth"
        ],
        "operationId": "Auth.SendEmailVerificationOTP",
        "parameters": [],
        "security": [],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$id": "/schemas/%7B%7D",
                  "anyOf": [
                    {
                      "type": "object"
                    },
                    {
