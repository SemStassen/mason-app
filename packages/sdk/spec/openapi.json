                ],
                "properties": {
                  "email": {
                    "type": "string",
                    "description": "a string matching the pattern ^[^@\\s]+@[^@\\s]+\\.[^@\\s]{2,}$",
                    "pattern": "^[^@\\s]+@[^@\\s]+\\.[^@\\s]{2,}$"
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "sign-in",
                      "email-verification",
                      "forget-password"
                    ]
                  }
                },
                "additionalProperties": false
